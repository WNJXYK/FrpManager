{
  "name": "frp-manager",
  "version": "1.0.0",
  "description": "FRP Manager for MacOS",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron .",
    "build": "rimraf dist && tsc -p tsconfig.electron.json && webpack",
    "dev": "concurrently \"tsc -p tsconfig.electron.json -w\" \"webpack --watch\" \"nodemon --watch dist/main --exec electron .\"",
    "watch": "concurrently \"tsc -p tsconfig.electron.json -w\" \"webpack --watch\"",
    "package": "npm run build && electron-builder build --mac"
  },
  "keywords": [
    "frp",
    "manager",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "build": {
    "appId": "dev.zhouz.frpmanager",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "assets/icons/icon.icns"
    },
    "files": [
      "dist/**/*"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "icon": "assets/icons/icon.png",
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@types/electron": "^1.4.38",
    "@types/node": "^18.0.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "babel-loader": "^9.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.0",
    "css-loader": "^6.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "html-webpack-plugin": "^5.0.0",
    "nodemon": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5"
  }
}
